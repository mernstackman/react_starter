{
  "name": "front",
  "version": "1.0.0",
  "description": "Starter kit for react front end development.",
  "main": "./dist/server.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "nodemon",
    "prod": "rimraf dist && webpack --mode production --config webpack.config.server.js && webpack --config webpack.config.prod.js",
    "prod-start": "rimraf dist && webpack --mode production --config webpack.config.server.js && webpack --config webpack.config.prod.js && node ./dist/server.js",
    "dev-build": "rimraf dist && webpack --mode development --config webpack.config.server.js && webpack --config webpack.config.dev.js",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "mern",
    "login system",
    "signup",
    "registration",
    "membership"
  ],
  "author": "Ari Susanto",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.0",
    "query-string": "^6.8.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "styled-components": "^4.3.2",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/addon-knobs": "^5.1.10",
    "@storybook/addon-notes": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "autoprefixer": "^9.4.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
