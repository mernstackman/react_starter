    "nodemon": "^1.18.10",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-cli": "^6.1.2",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "prettier-webpack-plugin": "^1.2.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "dead",
    "Firefox > 2"
  ]
}
